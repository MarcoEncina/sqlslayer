          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EndDataMB">
          <DataField>EndDataMB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EndLogMB">
          <DataField>EndLogMB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EndTotalMB">
          <Value>=Fields!EndDataMB.Value+Fields!EndLogMB.Value</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>SELECT
	EndSize.[ServerName]
	,EndSize.[DBName]
	,EndSize.DataSizeMB - StartSize.DataSizeMB AS [DataIncrease (MB)]
	,EndSize.LogSizeMB - StartSize.LogSizeMB AS [LogIncrease (MB)]
	,(EndSize.DataSizeMB + EndSize.LogSizeMB) - (StartSize.DataSizeMB + StartSize.LogSizeMB) AS [TotalIncrease (MB)]
	,EndSize.DataSizeMB AS EndDataMB
	,EndSize.LogSizeMB AS EndLogMB
FROM
(
	SELECT
		[ServerName] 
		,[DBName]
		,[DataSizeMB]
		,[LogSizeMB]
	FROM [DBACentral].[hist].[SpaceUsed_DatabaseSizes_vw]
	where ServerName IN (@ServerNames)
	and SampleDate between @StartDate AND DATEADD(DAY,1,@StartDate)
) AS StartSize
INNER JOIN
(
	SELECT
		[ServerName] 
		,[DBName]
		,[DataSizeMB]
		,[LogSizeMB]
	FROM [DBACentral].[hist].[SpaceUsed_DatabaseSizes_vw]
	WHERE ServerName IN (@ServerNames)
	AND SampleDate BETWEEN @EndDate AND DATEADD(DAY,1,@EndDate)
) AS EndSize
ON StartSize.ServerName = EndSize.ServerName
AND StartSize.DBName = EndSize.DBName
ORDER BY [TotalIncrease (MB)] DESC,[ServerName],[DBName]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ServerNames">
            <Value>=Parameters!ServerNames.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
