      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>SELECT 
	DISTINCT [ServerName]
FROM [hist].[SpaceUsed_DatabaseSizes_vw]
WHERE [SampleDate] BETWEEN @StartDate AND @EndDate
ORDER BY 1</CommandText>
        <QueryParameters>
