      <rd:DataSourceID>31f9afe3-ed3f-40d7-8f8f-0c7ad6e32855</rd:DataSourceID>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DataSizeGB">
          <DataField>DataSizeGB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="LogSizeGB">
          <DataField>LogSizeGB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="SampleDate">
          <DataField>SampleDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>SELECT
	db.[ServerName] 
	,SUM([DataSizeMB])/1024			AS [DataSizeGB]
	,SUM([LogSizeMB])	/1024			AS [LogSizeGB]
	,CAST([SampleDate] AS DATE)		AS [SampleDate]
FROM [hist].[SpaceUsed_DatabaseSizes_vw] db
INNER JOIN [dbo].[ServerInventory_SQL_AllServers_vw] svr
	ON db.[ServerName] = svr.[FullName]
where db.[ServerName] = @ServerName
and SampleDate between @StartDate AND @EndDate
GROUP BY db.ServerName, CAST([SampleDate] AS DATE)
ORDER BY [ServerName], CAST([SampleDate] AS DATE)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
