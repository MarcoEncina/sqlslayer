        </Field>
      </Fields>
      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>SELECT
	EndSize.[SchemaName]
	,EndSize.[TableName]
	,EndSize.[RowCount] - StartSize.[RowCount] AS [RowIncrease]
	,EndSize.[ReservedSpaceKB] - StartSize.[ReservedSpaceKB] AS [ReservedIncreaseKB]
	,EndSize.[DataSpaceKB] - StartSize.[DataSpaceKB] AS [DataIncreaseKB]
	,EndSize.[IndexSizeKB] - StartSize.[IndexSizeKB] AS [IndexIncreaseKB]
	,EndSize.[UnusedKB] - StartSize.[UnusedKB] AS [UnusedIncreaseKB]
	,EndSize.[RowCount] AS [LastRowCount]
	,EndSize.[ReservedSpaceKB] AS [LastReservedSpaceKB]
FROM
(
	SELECT
		[SchemaName]
		,[TableName]
		,[RowCount]
		,[ReservedSpaceKB]
		,[DataSpaceKB]
		,[IndexSizeKB]
		,[UnusedKB]
	FROM [hist].[SpaceUsed_TableSizes_vw]
	WHERE [ServerName] = @ServerName
	AND [DBName] = @DBName
	AND [SampleDate] BETWEEN @StartDate AND DATEADD(DAY,1,@StartDate)

) AS StartSize
INNER JOIN
(
	SELECT
		[SchemaName]
		,[TableName]
		,[RowCount]
		,[ReservedSpaceKB]
		,[DataSpaceKB]
		,[IndexSizeKB]
		,[UnusedKB]
	FROM [hist].[SpaceUsed_TableSizes_vw]
	WHERE [ServerName] = @ServerName
	AND [DBName] = @DBName
	AND [SampleDate] BETWEEN @EndDate AND DATEADD(DAY,1,@EndDate)
) AS EndSize
ON StartSize.[SchemaName] = EndSize.[SchemaName]
AND StartSize.[TableName] = EndSize.[TableName]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DBName">
            <Value>=Parameters!DBName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!ReservedIncreaseKB.Value+Fields!RowIncrease.Value+Fields!DataIncreaseKB.Value+Fields!IndexIncreaseKB.Value+Fields!UnusedIncreaseKB.Value</FilterExpression>
          <Operator>NotEqual</Operator>
          <FilterValues>
            <FilterValue DataType="Integer">0</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
