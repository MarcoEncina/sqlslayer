        </Field>
        <Field Name="StartDataMB">
          <DataField>Start Data MB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="StartLogMB">
          <DataField>Start Log MB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EndDataMB">
          <DataField>Ending Data MB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="EndLogMB">
          <DataField>Ending Log MB</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DataIncreaseMB">
          <Value>=Fields!EndDataMB.Value-Fields!StartDataMB.Value</Value>
        </Field>
        <Field Name="LogIncreaseMB">
          <Value>=Fields!EndLogMB.Value-Fields!StartLogMB.Value</Value>
        </Field>
        <Field Name="TotalIncreaseMB">
          <Value>=(Fields!EndDataMB.Value-Fields!StartDataMB.Value)+(Fields!EndLogMB.Value-Fields!StartLogMB.Value)</Value>
        </Field>
        <Field Name="EndTotalMB">
          <Value>=Fields!EndDataMB.Value+Fields!EndLogMB.Value</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>
SELECT
	EndSize.[ServerName]
	,StartSize.DataSizeMB			AS [Start Data MB]
	,StartSize.LogSizeMB			AS [Start Log MB]
	,EndSize.DataSizeMB				AS [Ending Data MB]
	,EndSize.LogSizeMB				AS [Ending Log MB]
FROM
(
	SELECT
		db.[ServerName] 
		,SUM([DataSizeMB])			AS [DataSizeMB]
		,SUM([LogSizeMB])			AS [LogSizeMB]
	FROM [hist].[SpaceUsed_DatabaseSizes_vw] db
	INNER JOIN [dbo].[ServerInventory_SQL_AllServers_vw] svr
		ON db.[ServerName] = svr.[FullName]
	where svr.[Environment] IN (@Environment)
	and SampleDate between @StartDate AND DATEADD(DAY,1,@StartDate)
	GROUP BY db.[ServerName]
) AS StartSize
INNER JOIN
(
	SELECT
		db.[ServerName] 
		,SUM([DataSizeMB])			AS [DataSizeMB]
		,SUM([LogSizeMB])			AS [LogSizeMB]
	FROM [hist].[SpaceUsed_DatabaseSizes_vw] db
	INNER JOIN [dbo].[ServerInventory_SQL_AllServers_vw] svr
		ON db.[ServerName] = svr.[FullName]
	WHERE svr.[Environment] IN (@Environment)
	AND SampleDate BETWEEN @EndDate AND DATEADD(DAY,1,@EndDate)
	GROUP BY db.[ServerName]
) AS EndSize
ON StartSize.ServerName = EndSize.ServerName
ORDER BY [ServerName]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Environment">
            <Value>=Parameters!Environment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
