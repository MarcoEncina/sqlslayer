  <DataSets>
    <DataSet Name="DeadlockServerSummary">
      <Fields>
        <Field Name="Ct">
          <DataField>Ct</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Dt">
          <DataField>Dt</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DBACentral</DataSourceName>
        <CommandText>SELECT 
	COUNT(*) AS Ct
	,CAST([DeadlockDate] AS DATE) AS Dt
FROM [hist].[Deadlock_Deadlocks_vw]
WHERE [ServerName] = @ServerName
AND [DeadlockDate] BETWEEN @StartDate AND @EndDate
GROUP BY CAST([DeadlockDate] AS DATE)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
